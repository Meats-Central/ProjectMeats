# React Environment Variables for ProjectMeats3
# Copy to .env.local and update with your specific values

# API Configuration
REACT_APP_API_BASE_URL=http://localhost:8000/api/v1

# Environment
REACT_APP_ENVIRONMENT=development

# AI Assistant Configuration
REACT_APP_AI_ASSISTANT_ENABLED=true

# Features
REACT_APP_ENABLE_DOCUMENT_UPLOAD=true
REACT_APP_ENABLE_CHAT_EXPORT=true

# UI Configuration
REACT_APP_MAX_FILE_SIZE=10485760
REACT_APP_SUPPORTED_FILE_TYPES=pdf,jpg,jpeg,png,txt,doc,docx,xls,xlsx