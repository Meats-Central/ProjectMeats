# Vite Production Environment Variables

# API Configuration
# Will be set by deployment pipeline or env-config.js

# Environment
VITE_ENVIRONMENT=production

# AI Assistant Configuration
VITE_AI_ASSISTANT_ENABLED=true

# Features (Production settings)
VITE_ENABLE_DOCUMENT_UPLOAD=true
VITE_ENABLE_CHAT_EXPORT=true

# UI Configuration
VITE_MAX_FILE_SIZE=10485760
VITE_SUPPORTED_FILE_TYPES=pdf,jpg,jpeg,png,txt,doc,docx,xls,xlsx

# Production Features (Disabled for security)
VITE_ENABLE_DEBUG=false
VITE_ENABLE_DEVTOOLS=false
